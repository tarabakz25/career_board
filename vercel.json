{
	"version": 2,
	"builds": [
		{
			"src": "index.ts",
			"use": "@vercel/node"
		},
		{
			"src": "public/**",
			"use": "@vercel/static"
		}
	],
	"routes": [
		{
			"src": "/(.*)\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot)",
			"dest": "/public/$1.$2"
		},
		{
			"src": "/(.*)",
			"dest": "/index.ts"
		}
	],
	"env": {
		"NODE_ENV": "production"
	}
}
